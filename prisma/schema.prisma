generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model files {
  id         Int     @id @default(autoincrement())
  status     String? @default("unaanotated") @db.VarChar(255)
  audioname  String? @db.LongText
  transcript String? @db.LongText
}
